public class 1 {
    private int x;
    private int y;

    public Point() {
        x = 0;
        y = 0;
    }

    public Point(int theX, int theY) {
        x = theX;
        y = theY;
    }

    public int getX() {
        return x;
    }

    public int getY() {
        return y;
    }

    public void move(int newX, int newY) {
        x = newX;
        y = newY;
    }

    public String toString() {
        return "(" + x + ", " + y + ")";
    }
}
